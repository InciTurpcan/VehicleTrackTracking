@model CreatePartCategoryDto
@{
    ViewData["Title"] = "CreatePartCategory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page-content">
    <div class="container-fluid">

        <!-- page content -->
        <div class="row">
            <div class="col-xl-12">

                <div class="card">
                    <div class="card-body">

                        <h4 class="card-title">Add New PartCategory</h4>

                        <form method="post">
                            <div class="form-group">
                                <label for="exampleInputPlatePartCategoryName">PartCategory Name</label>
                                <input type="text" class="form-control" asp-for="PartCategoryName" id="exampleInputPartCategoryName" placeholder="">
                            </div><br />
                        
                            <button type="submit" class="btn btn-primary waves-effect waves-light">Save</button>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

